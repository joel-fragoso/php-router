{
  "name": "joel-fragoso/router",
  "description": "A simple PHP router",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Joel Fragoso",
      "email": "joelfragoso85@icloud.com"
    }
  ],
  "minimum-stability": "stable",
  "scripts": {
    "start": "php -S localhost:3333 -t public/ public/index.php",
    "test": "phpunit --colors=always"
  },
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Prix\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Prix\\Tests\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  }
}
